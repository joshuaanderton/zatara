{
    "name": "zatara/zatara",
    "description": "Zero-config client implementation for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Anderton",
            "email": "hey@joshuaanderton.ca"
        }
    ],
    "require": {
        "php": "^8.1|^8.2"
    },
    "autoload": {
        "psr-4": {
            "Zatara\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zatara\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zatara\\Providers\\ServiceProvider"
            ]
        }
    }
}
